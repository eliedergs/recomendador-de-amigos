{
    "name": "recomendador-amigos",
    "version": "0.0.1",
    "description": "Um sistema que permite que uma pessoa obtenha sugestões de novos amigos se baseando em amizades já existentes.",
    "scripts": {
        "start": "ts-node -r tsconfig-paths/register --transpile-only src/server.ts",
        "dev": "ts-node-dev -r tsconfig-paths/register --respawn --transpile-only --ignore-watch node_modules src/server.ts",
        "clean": "rimraf coverage build tmp",
        "build": "tsc -p tsconfig.json",
        "lint": "eslint . --ext .ts",
        "test": "yarn jest -i",
        "prettier": "prettier --config .prettierrc --write .",
        "test:watch": "jest --watch"
    },
    "dependencies": {
        "class-transformer": "^0.5.1",
        "class-validator": "^0.13.2",
        "cors": "^2.8.5",
        "express": "^4.17.2",
        "express-async-errors": "^3.1.1",
        "lodash": "^4.17.21",
        "luxon": "^2.3.1",
        "reflect-metadata": "^0.1.13",
        "swagger-ui-express": "^4.3.0"
    },
    "devDependencies": {
        "@types/cors": "^2.8.12",
        "@types/express": "^4.17.13",
        "@types/jest": "^27.4.1",
        "@types/lodash": "^4.14.181",
        "@types/luxon": "^2.3.1",
        "@types/node": "^17.0.16",
        "@types/supertest": "^2.0.12",
        "@types/swagger-ui-express": "^4.1.3",
        "@typescript-eslint/eslint-plugin": "^5.36.1",
        "@typescript-eslint/parser": "^5.36.1",
        "eslint": "^8.8.0",
        "eslint-config-google": "^0.14.0",
        "jest": "^27.5.1",
        "jest-mock-extended": "^2.0.5",
        "supertest": "^6.2.2",
        "ts-jest": "^27.1.4",
        "ts-node-dev": "^1.1.8",
        "tsconfig-paths": "^3.12.0",
        "tscpaths": "^0.0.9",
        "typescript": "^4.5.5"
    },
    "author": "Elieder Gomes <https://github.com/eliedergs>",
    "license": "Apache-2.0"
}
